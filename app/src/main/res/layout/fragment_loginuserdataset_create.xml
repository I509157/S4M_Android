                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/systemnbrid"
                        android:tag="SystemNbr"
                        android:maxLength="@{18}"
                        app:key="SystemNbr"
                        app:value="@={loginUserData.systemNbr}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/systemtxtid"
                        android:tag="SystemTxt"
                        android:maxLength="@{60}"
                        app:key="SystemTxt"
                        app:value="@={loginUserData.systemTxt}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/timezoneid"
                        android:tag="TimeZone"
                        android:maxLength="@{6}"
                        app:key="TimeZone"
                        app:value="@={loginUserData.timeZone}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/timezoneoffsetid"
                        android:tag="TimeZoneOffset"
                        app:key="TimeZoneOffset"
                        app:value="@={Converter.toString(timezoneoffsetid, loginUserData.timeZoneOffset, loginUserData.timeZoneOffset)}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
